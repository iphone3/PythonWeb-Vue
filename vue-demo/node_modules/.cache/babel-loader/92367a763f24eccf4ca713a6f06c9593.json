texport default {\n\t\tdata(){\n\t\t\treturn{\n\t\t\t\tname1: 'Atom',\n\t\t\t\tname2: '阿童木',\n\t\t\t\tmsg: 'H<sub>2</sub>O',\n\t\t\t\tisShow: false,\n\t\t\t\tnames: [\n\t\t\t\t\t'张三',\n\t\t\t\t\t'李四',\n\t\t\t\t\t'王五',\n\t\t\t\t\t'赵六'\n\t\t\t\t],\n\t\t\t\tobjs: {\n\t\t\t\t\tname: '老王',\n\t\t\t\t\tdetail: '你有事情我帮忙，我住隔壁我姓王!',\n\t\t\t\t\tscore: 99,\n\t\t\t\t\tage:18\n\t\t\t\t},\n\t\t\t\tcolor: 'blue',\n\t\t\t\tisRed: true,\t// 修改true/false改变对应样式\n\t\t\t\tdetail: 'hello vue'\n\t\t\t}\n\t\t},\n\t\tmethods:{\n\t\t\tshowTitle: function(){\n\t\t\t\talert('很乖哦!')\n\t\t\t},\n\t\t\tshowContent: function(){\n\t\t\t\talert('你傻啊?')\n\t\t\t},\n\t\t}\n\t}\n</script>\n\n<style>\n\t.red{\n\t\tcolor: red;\n\t}\n\t.blue{\n\t\tcolor: blue;\n\t}\n\t\n\t.active{\n\t\twidth: 100%;\n\t\theight: 30px;\n\t\tbackground: red;\n\t\tcolor: white;\n\t\tline-height: 30px;\n\t\ttext-align: center;\n\t}\n</style>\n"],"sourceRoot":"src/components"}]}